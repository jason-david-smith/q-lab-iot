{
  "name": "q-lab-iot",
  "version": "0.0.1",
  "description": "Web Application for Thoughtworks Atlanta's IOT Lab",
  "main": "index.js",
  "scripts": {
    "webserver": "node api/src/server.js",
    "build": "./node_modules/.bin/webpack",
    "dev": "./node_modules/.bin/webpack-dev-server",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --require app/test/test_helper.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run tests -- --watch",
    "lint": "./node_modules/.bin/eslint . --ext .js --ext .jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jason-david-smith/q-lab-iot.git"
  },
  "keywords": [
    "iot",
    "koa",
    "react",
    "redux"
  ],
  "author": "Jason Smith (jason.smith@thoughtworks.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jason-david-smith/q-lab-iot/issues"
  },
  "homepage": "https://github.com/jason-david-smith/q-lab-iot#readme",
  "dependencies": {
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "html-webpack-plugin": "^1.7.0",
    "isomorphic-fetch": "^2.2.0",
    "koa": "^1.1.2",
    "koa-cors": "0.0.16",
    "koa-route": "^2.4.2",
    "react": "^0.14.3",
    "react-loader": "^2.0.0",
    "react-redux": "^4.0.1",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.2",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.12.0",
    "jsdom": "^7.2.1",
    "mocha": "^2.3.4",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.14.0"
  }
}
